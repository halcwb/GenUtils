        <Compile Include="NullCheck.fs" />
        <Compile Include="BCL\Char.fs" />
        <Compile Include="BCL\String.fs" />
        <Compile Include="BCL\Int32.fs" />
        <Compile Include="BCL\Double.fs" />
        <Compile Include="BCL\BigRational.fs" />
        <Compile Include="BCL\DateTime.fs" />
        <Compile Include="Option.fs" />
        <Compile Include="Array.fs" />
        <Compile Include="List.fs" />
        <Compile Include="Seq.fs" />
        <Compile Include="Path.fs" />
        <Compile Include="File.fs" />
        <Compile Include="App.fs" />
        <None Include="Scripts\Script.fsx" />
    </ItemGroup>
    <Import Project="..\..\.paket\Paket.Restore.targets" />
</Project>
